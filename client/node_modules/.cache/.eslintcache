ken', null)\r\n        console.log('get token')\r\n        console.log(res)\r\n        dispatch({type: 'GET_TOKEN', payload: res.data.access_token})\r\n      }\r\n      getToken()\r\n    }\r\n  },[auth.isLogged,dispatch])\r\n\r\n  useEffect(() => {\r\n    if(token){\r\n      const getUser = () => {\r\n        dispatch(dispatchLogin())\r\n\r\n        return fetchUser(token).then(res => {\r\n          dispatch(dispatchGetUser(res))\r\n        })\r\n      }\r\n      getUser()\r\n    }\r\n  },[token, dispatch])\r\n\r\n  const headerUser = () =>{\r\n    return <>\r\n      <MainPages />\r\n    </>\r\n  }\r\n\r\n  const headerAdmin = () =>{\r\n    return <>\r\n    <Layout style={{padding: 0 ,minHeight: '100vh' }}>\r\n      <SiderBar/>\r\n      <Layout className=\"site-layout\" style={{padding: 0, minHeight: '100vh' }}>\r\n        <HeaderAdmin/>\r\n        <Content style={{ margin: '24px 16px 0', overflow: 'initial' }}>\r\n          <div className=\"site-layout-background\" style={{ padding: 24, minHeight: 500 }}>\r\n            <MainPages />\r\n          </div>\r\n        </Content>\r\n      </Layout>\r\n    </Layout>\r\n    </>\r\n  }\r\n\r\n  return (\r\n    <DataProvider>\r\n      <Router>\r\n      <div className=\"App\">\r\n        {isAdmin?headerAdmin():headerUser()}\r\n      </div>\r\n      </Router>\r\n    </DataProvider>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","D:\\Documents\\HKII_2020_2021\\Tieu_Luan_CN\\code\\tlcn\\client\\src\\components\\redux\\store.js",[],"D:\\Documents\\HKII_2020_2021\\Tieu_Luan_CN\\code\\tlcn\\client\\src\\GlobalState.js",["280","281","282","283","284"],"import React, {createContext, useState, useEffect} from 'react'\r\nimport UserAPI from './api/UserAPI'\r\nimport CategoriesAPI from './api/CategoriesAPI'\r\nimport ProductsAPI from './api/ProductsAPI'\r\nimport TypesAPI from './api/TypesAPI'\r\nimport StatusesAPI from './api/StatusAPI.js'\r\nimport PetsAPI from './api/PetsAPI'\r\nimport MyPostsAPI from './api/MyPostsAPI'\r\nimport TopicsAPI from './api/TopicsAPI'\r\nimport {useDispatch, useSelector} from 'react-redux'\r\nimport axios from 'axios';\r\nexport const GlobalState = createContext()\r\n\r\n\r\nexport const DataProvider = ({children}) =>{\r\n\r\n     const token = useSelector(state => state.token)\r\n     const auth = useSelector(state => state.auth)\r\n     \r\n\r\n     const state = {\r\n        token: token,\r\n        productsAPI: ProductsAPI(),\r\n        userAPI: UserAPI(token),\r\n        categoriesAPI: CategoriesAPI(),\r\n        typesAPI : TypesAPI(),\r\n        statusesAPI: StatusesAPI(),\r\n        petsAPI: PetsAPI(),\r\n        myPostsAPI: MyPostsAPI(token),\r\n        topicsAPI: TopicsAPI()\r\n    } \r\n\r\n\r\n    return(\r\n        <GlobalState.Provider value={state}>\r\n            {children}\r\n        </GlobalState.Provider>\r\n    )\r\n}\r\n","D:\\Documents\\HKII_2020_2021\\Tieu_Luan_CN\\code\\tlcn\\client\\src\\components\\headers\\Header.js",["285","286","287","288","289"],"\r\nimport React, {useEffect,useContext} from 'react';\r\nimport {Link} from 'react-router-dom';\r\nimport { Menu, Dropdown,Avatar, Image,Badge } from 'antd';\r\nimport 'antd/dist/antd.css';\r\nimport { LoginOutlined,TeamOutlined,ShoppingCartOutlined,UserOutlined } from '@ant-design/icons';\r\nimport {GlobalState} from '../../GlobalState';\r\nimport {useSelector} from 'react-redux'\r\nimport { createFromIconfontCN } from '@ant-design/icons';\r\nimport axios from 'axios'\r\n\r\n\r\nfunction Header(){\r\n\r\n   const state = useContext(GlobalState)\r\n    console.log(state)\r\n    const auth = useSelector(state => state.auth)\r\n    const {user, isLogged} = auth\r\n\r\n\r\n    const handleLogout = async () => {\r\n      try {\r\n          await axios.get('/user/logout')\r\n          localStorage.removeItem('firstLogin')\r\n          localStorage.removeItem('token')\r\n          window.location.href = \"/\";\r\n      } catch (err) {\r\n          window.location.href = \"/\";\r\n      }\r\n  }\r\n    